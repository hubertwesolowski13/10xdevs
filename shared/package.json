{
  "name": "shared",
  "version": "1.0.2",
  "description": "Shared isomorphic code (types and small utilities) for API and App",
  "license": "ISC",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./src/*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "src/*": [
        "dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prepare": "pnpm run build"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@types/express": "^5.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2"
  }
}
