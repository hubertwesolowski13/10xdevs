{
  "name": "wardrobe-assistant",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start:app": "pnpm run --filter wardrobe-assistant-app expo start",
    "start:app:web": "pnpm run --filter wardrobe-assistant-app web",
    "start:app:android": "pnpm run --filter wardrobe-assistant-app android",
    "start:app:ios": "pnpm run --filter wardrobe-assistant-app ios",
    "start:api": "pnpm run --filter wardrobe-assistant-api start:dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:app": "eslint wardrobe-assistant-app",
    "lint:api": "eslint wardrobe-assistant-api",
    "format": "prettier --config ./.prettierrc --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --config ./.prettierrc --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "db:init": "supabase init",
    "db:start": "supabase start",
    "db:migrate": "supabase migration up",
    "db:anon": "supabase status -o env",
    "db:generate-types": "supabase gen types typescript --local > shared/src/types/database.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "supabase": "^2.54.11",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.20.0"
  },
  "packageManager": "pnpm@9.12.0"
}
